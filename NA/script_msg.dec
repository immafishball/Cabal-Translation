		<msg id="20795" msg_uid="158GUARD__0DF_1_4_158_17" cont="Report Merit Medal" />
		<msg id="20796" msg_uid="158GUARD__0DF_1_0_158_18" cont="Welcome to aboard $32#Spada Brillante$. \" />
		<msg id="20797" msg_uid="158GUARD__0DF_1_1_158_18" cont="Open the Store" />
		<msg id="20798" msg_uid="158GUARD__0DF_1_2_158_18" cont="Let's just pass" />
