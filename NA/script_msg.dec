		<msg id="1226" msg_uid="00001026_0_1ST_1_0_13_3" cont="The sealed wall has opened and the warp gate has been activated. \Enter the gate. \" />
		<msg id="1227" msg_uid="00001026_0_1ST_1_1_13_3" cont="I am moving to the room of the Werewolf." />
		<msg id="1228" msg_uid="00001026_0_2NG_1_0_13_3" cont="The sealed wall has opened and the warp gate has been activated. \Enter the gate. \" />
		<msg id="1229" msg_uid="00001026_0_2NG_1_1_13_3" cont="I am moving to the room of the Werewolf." />
		<msg id="1230" msg_uid="00001026_1_4CR_1_0_13_94" cont="A blue lamp shines in the center of the room. \Insert the Werewolf stone into the lamp. \" />
		<msg id="1231" msg_uid="00001026_1_4CR_1_1_13_94" cont="Insert the stone into the lamp." />
